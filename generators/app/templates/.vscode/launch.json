    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/server.js",
            "env": {
                "FORGE_CLIENT_ID": "<%= forge_client_id %>",
                "FORGE_CLIENT_SECRET": "<%= forge_client_secret %>"
            }
        }
    ]
}